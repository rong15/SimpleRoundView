<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 声明属性级的名称 -->
    <declare-styleable name="zrCircleView">

        <!-- 声明一个属性，整型 -->
        <attr name="circle_color" format="color" />
        <!-- 声明一个属性，字符串 -->
        <attr name="circle_width" format="dimension" />
        <!-- 声明一个属性，引用，引用资源id -->
        <attr name="default_image" format="reference" />
    </declare-styleable>

</resources>